%html{lang: "en"}
  %head
    %meta{content: "text/html; charset=UTF-8", "http-equiv" => "content-type"}/
    %meta{charset: "utf-8"}/
    %title {{ site.name }}
    %meta{content: "width=device-width, initial-scale=1, maximum-scale=1", name: "viewport"}/

    {{ 'application.css' | stylesheet_tag }}
    {{ 'carousel.css' | stylesheet_tag }}
    {{ 'gcfmalaysia-template.css' | stylesheet_tag }}

    /[if lt IE 9]
      <script src="https://html5shim.googlecode.com/svn/trunk/html5.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    %link{rel: "shortcut icon", href: "/samples/favicon.png"}/
    %link{rel: "apple-touch-icon-precomposed", sizes: "144x144", href: "/samples/apple-touch-icon-144x144-precomposed.png"}/
    %link{rel: "apple-touch-icon-precomposed", sizes: "114x114", href: "/samples/apple-touch-icon-114x114-precomposed.png"}/
    %link{rel: "apple-touch-icon-precomposed", sizes: "72x72", href: "/samples/apple-touch-icon-72x72-precomposed.png"}/
    %link{rel: "apple-touch-icon-precomposed", href: "/samples/apple-touch-icon-precomposed.png"}/
    %link{rel: "stylesheet", type: "text/css", href: "http://fonts.googleapis.com/css?family=Lobster"}

    {% inline_editor %}

  %body

    {% include "navbar" %}

    /
      Carousel
      \==================================================
    #myCarousel.carousel
      .carousel-inner{role: "listbox"}
        {% block 'carousel' %}
        / .item.active.carousel-background{style: "background-image: url('{{ 'banner-christmas-web.jpg' | theme_image_url }}')"}
        /   .container
        /     .carousel-caption
        /       %h1.font-lobster East Asia Graduate Conference
        /       %p Mobilise graduates to impact society for the glory of Christ
        {% endblock %}
      .carousel-caption-background

    / /.carousel

    /
      Content
      \==================================================
    .container
      .row.featurette
        .col-md-12
          {% block 'main' %}
          {% block 'header' %}
          / %h2.featurette-heading mobilise graduates to impact <br/><span class="text-muted">society for the glory of Christ</span>
          {% endblock %}
          {% block 'content' %}
          / %p.lead “Call to me and I will answer you and tell you great and unsearchable things you do not know.” -Jeremiah 33:3
          {% endblock %}
          {% block 'more-content' %}
          / %p.lead “Call to me and I will answer you and tell you great and unsearchable things you do not know.” -Jeremiah 33:3
          {% endblock %}
          {% endblock %}
    / /.content

    /
      Event
      \==================================================
    {% block 'event' %}
    .featurette-divider
    #myCarousel2.carousel
      .carousel-inner{role: "listbox"}
        {% block 'carousel' %}
        / .item.active.carousel-background{style: "background-image: url('{{ 'cny2015-street-simple.jpg' | theme_image_url }}')"}
        /   .container
        /     .carousel-caption
        /       %h1.font-lobster {% editable_text "Event Title", hint: "event title", format: raw, rows: 1 %}iBridge Chinese New Year Gathering{% endeditable_text %}
        /       %p {% editable_text "Event Description", hint: "event description", format: raw, rows: 1 %}FEBRUARY 28, 2015{% endeditable_text %}
        /       %a.btn.btn-success{role: "button", href: "https://www.facebook.com/events/556774091092360"} JOIN US
        {% endblock %}
      .carousel-caption-background
    {% endblock %}
    / /.event


    {% include "footer" %}


    /
      Bootstrap 3 JS
      \==================================================
    %script{src: "//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js", type: "text/javascript"}
    {{ 'bootstrap.min.js' | javascript_tag }}
    {{ 'bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js' | javascript_tag }}
    :javascript
      $(document).ready(function() {
      });
    / /.js